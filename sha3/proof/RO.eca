type from, to.

module type RO = {
  proc init()     : unit
  proc f(x : from): to
}.

module type RO_ = {
  proc f(x : from): to
}.

module type Distinguisher(G : RO_) = {
  proc distinguish(): bool
}.

module IND(G:RO, D:Distinguisher) = {
  proc main(): bool = {
    var b;

         G.init();
    b <@ D(G).distinguish();
    return b;
  }  
}.
